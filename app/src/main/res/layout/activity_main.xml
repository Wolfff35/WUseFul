<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/fragment_container_main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.wolff.wuseful.MainActivity">
    <com.wolff.wuseful.custom_view.ValueSelector
        android:id="@+id/valueSelector"
        android:layout_width="match_parent"
        android:layout_height="@dimen/valueSelector_height">
    </com.wolff.wuseful.custom_view.ValueSelector>

      <com.wolff.wuseful.custom_view.DurationTextView
        android:id="@+id/durationview1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:template="%s пробежали"/>

    <com.wolff.wuseful.custom_view.DurationTextView
        android:id="@+id/durationview2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:template="%s не пробежали"/>

    <com.wolff.wuseful.custom_view.DurationTextView
        android:id="@+id/durationview3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />

    <com.wolff.wuseful.custom_view.DurationTextView
        android:id="@+id/durationview4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />

    <com.wolff.wuseful.custom_view.DurationTextView
        android:id="@+id/durationview5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />

    <com.wolff.wuseful.custom_view.CompassView
        android:id="@+id/compassView"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:visibility="invisible" />


</LinearLayout>
